name                    = "psdmo"
region                  = "us-ashburn-1"
compartment_ocid        = "ocid1.compartment.xxx"
subnet_id               = "ocid1.subnet.xxx"
availability_domain     = 1
fault_domain            = 1
backup_policy           = "silver"
shape                   = "VM.Standard2.1"
instance_number         = "01"
storage                 = "150"
enable_public_ip        = true
preserve_boot_volume    = false
ssh_public_key          = "ssh-rsa xxx"

# Optional if TF_VAR_tenancy_ocid env variable is set
tenancy_ocid            = "ocid1.tenancy.xxx"

# Action for ioco: 
#   cloud-init is required and first
#   oci will mount block storage
#   cm will attach Cloud Manager repository
#   dpk will run the PeopleSoft dpk
#   configuration options for ioco are set in config.json
ioco_actions            = [ "cloud-init", "oci", "cm", "dpk" ]